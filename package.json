    "babel-preset-es2015": "^6.6.0",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.14.1",
    "buildreq": "latest",
    "chalksay": "^1.1.0",
    "compression": "^1.6.0",
