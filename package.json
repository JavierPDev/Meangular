{
  "name": "meangular",
  "version": "0.30.3",
  "description": "Open-source MEAN stack solution based on Mean Stack JS and Angular-cli for Angular (2+).",
  "main": "index.js",
  "engines": {
    "node": "7.10.1",
    "npm": "3.10.9"
  },
  "standard": {
    "globals": [
      "angular",
      "toastr",
      "moment",
      "self",
      "_",
      "describe",
      "it",
      "__dirname",
      "localStorage",
      "io",
      "expect",
      "inject",
      "beforeEach",
      "before",
      "beforeAll",
      "afterEach",
      "after",
      "afterAll"
    ],
    "ignore": [
      "commands/template/",
      "commands/blank/",
      "database/",
      "downloads/",
      "backups/",
      "tools/"
    ]
  },
  "scripts": {
    "browser-sync": "browser-sync start --proxy 'localhost:3000' --port 3001 --files 'client/dist/inline.bundle.js' 'client/dist/styles.bundle.css' 'client/dist/main.bundle.js' --no-open",
    "build:dev": "ng build",
    "build:prod": "ng build --prod --aot",
    "build:watch": "ng build --watch",
    "cli": "node commands/cli.js",
    "e2e": "npm run build:prod && protractor",
    "e2e:dev": "protractor",
    "generate-ssl-certs": "bash scripts/generate-ssl-certs.sh ",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "mocha": "mocha mocha.test.js  ",
    "ng": "ng",
    "postinstall": "node scripts/postinstall.js && webdriver-manager update",
    "processes:linux": "ps -ef | grep 'node index.js'",
    "processes:linux:kill": "ps -ef | grep 'node index.js' | grep -v grep | awk '{print $2}' | xargs kill -9",
    "seed": "node seed/seed.js",
    "start": "concurrently -k \"npm run build:watch\" \"npm run start:server:watch\"",
    "start:debug": "node debug index.js",
    "start:inspect": "node --inspect  index.js",
    "start:prod": "npm run build:prod && cross-env NODE_ENV=production node index.js",
    "start:server": "node index.js",
    "start:server:watch": "cross-env NODE_ENV=development nodemon index.js",
    "start:test": "cross-env NODE_ENV=test node index.js",
    "test": "npm run mocha && ng test --single-run && npm run e2e && npm run lint && ng lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JavierPDev/Meangular.git"
  },
  "homepage": "https://github.com/JavierPDev/Meangular#readme",
  "keywords": [
    "Meangular",
    "Mean",
    "Mean Stack",
    "Mean Stack JS",
    "Javascript framework",
    "Mongodb",
    "Expressjs",
    "Express",
    "Angular",
    "Node",
    "Nodejs",
    "Meanstackjs"
  ],
  "author": "Javier <javier@javierdev.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JavierPDev/Meangular/issues"
  },
