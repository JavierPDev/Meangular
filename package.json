{
  "name": "meangular",
  "version": "0.17.0",
  "description": "Meangular is built on Mean Stack JS and Angular-cli",
  "main": "index.js",
  "engines": {
    "node": "7.1.0",
    "npm": "3.10.9"
  },
  "standard": {
    "globals": [
      "angular",
      "toastr",
      "moment",
      "self",
      "_",
      "describe",
      "it",
      "__dirname",
      "localStorage",
      "io",
      "expect",
      "inject",
      "beforeEach",
      "before",
      "afterEach",
      "after"
    ],
    "ignore": [
      "commands/template/",
      "commands/blank/",
      "database/",
      "downloads/",
      "backups/",
      "tools/"
    ]
  },
  "scripts": {
    "pree2e": "npm run ng:build:prod",
    "e2e": "protractor",
    "e2e:dev": "protractor",
    "test": "npm run mocha && ng test --single-run && npm run e2e && npm run lint && ng lint",
    "mocha": "node_modules/.bin/mocha tests/unit/mocha.test.js  ",
    "lint": "node_modules/.bin/standard",
    "lint:fix": "node_modules/.bin/standard --fix",
    "postinstall": "node scripts/postinstall.js && webdriver-manager update",
    "ng": "ng",
    "ng:build:watch": "node_modules/.bin/ng build --watch",
    "ng:build:prod": "node_modules/.bin/ng build --prod --aot",
    "start": "concurrently -k \"npm run ng:build:watch\" \"npm run start:server:watch\"",
    "browser-sync": "browser-sync start --proxy 'localhost:3000' --port 3001 --files 'client/dist/inline.bundle.js' 'client/dist/styles.bundle.css' 'client/dist/main.bundle.js' --no-open",
    "start:debug": "node debug index.js",
    "start:inspect": "node --inspect  index.js",
    "start:server": "node index.js",
    "start:server:watch": "node_modules/.bin/cross-env NODE_ENV=development node_modules/.bin/nodemon index.js",
    "start:test": "node_modules/.bin/cross-env NODE_ENV=test node index.js",
    "start:production": "node_modules/.bin/cross-env NODE_ENV=production && npm run ng:build:prod && node index.js",
    "cli": "node commands/cli.js",
    "generate-ssl-certs": "bash scripts/generate-ssl-certs.sh ",
    "processes:linux": "ps -ef | grep 'node index.js'",
    "processes:linux:kill": "ps -ef | grep 'node index.js' | grep -v grep | awk '{print $2}' | xargs kill -9",
    "selenium:install": "node_modules/.bin/selenium-standalone install",
    "selenium:start": "node_modules/.bin/selenium-standalone start",
    "update": "node scripts/update-meanstackjs.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greenpioneersolutions/meanstackjs.git"
  },
  "homepage": "https://github.com/greenpioneersolutions/meanstackjs#readme",
  "keywords": [
    "Meangular",
    "Mean",
    "Mean Stack",
    "Mean Stack JS",
    "Javascript framework",
    "Mongodb",
    "Expressjs",
    "Express",
    "Angular",
    "Angularjs",
    "Node",
    "Nodejs",
    "Meanstackjs",
    "Greenpioneersolutions",
    "Green Pioneer"
  ],
  "author": "Javier <javier@javierdev.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JavierPDev/Meangular/issues"
  },
  "dependencies": {
    "@angular/cli": "^1.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
